import java.util.Scanner;

public class Jung {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("커피종류");
		String coffee = sc.next();
		switch(coffee) {
		case "에스프레소":
			System.out.println("에스프레소 나왔심더");	break;
		case "아메리카노":
			System.out.println("아메리카노 나왔심더");	break;
		}
		sc.close();
	}

}
