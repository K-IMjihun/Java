
public class Tutorial222 {

	public static void main(String[] args) {

		int a = 1;
		int b = 2;
		
		System.out.println("a + b = " + (a + b));
		System.out.println("a - b = " + (a - b));
		System.out.println("a * b = " + (a * b));
		System.out.println("a / b = " + (a / b));
		
//		사칙연산 하는법, 나누기는 몫만 나옴
		

	}

}
