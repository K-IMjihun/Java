
public class Book {
	String title, author;
	void show() {
		System.out.print(title + " " + author);

	}
	public Book() {
		this("","");
		System.out.println("생성자 호출됨");
	}
	public Book(String title) {
		this(title,"작자미상");
	}
	public Book(Stinrg title, String author) {
		this.title = title;
		this.author = author;
	}
	public static void main(String[] args) {
	Book loveStory = new Book();
	loveStory.show();

	}

}
